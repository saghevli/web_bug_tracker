{
  "name": "Web Bug Tracker",
  "version": "1.1",
  "description": "A browser action with a popup dump of Google Analytics web bugs",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*",
    "cookies"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
      "default_title": "Web Bug Tracker",
      "default_icon": "icon.png"      
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}