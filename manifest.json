{
  "name": "Web Bug Tracker",
  "version": "1.1",
  "description": "A browser action with a popup dump of Google Analytics web bugs",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
      "default_title": "Web Bug Tracker",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}